{
  "name": "actograph-v3-api",
  "version": "0.0.88",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "generator": "ts-node -T --project ./tsconfig.json generator/run.ts",
    "prebuild": "rimraf dist",
    "build": "nest build && tsc-alias -p tsconfig.build.json",
    "generate:indexes": "node scripts/generate-indexes.js",
    "build:bundle": "node scripts/generate-indexes.js && node esbuild.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch ",
    "start:dev-electron": "DEV_ELECTRON=true ts-node-dev --respawn --transpile-only --project ./tsconfig.json -r tsconfig-paths/register src/main.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/api/src/main",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && yarn format",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node -T --project ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:cli-with-configs": "yarn typeorm:cli -d ./config/migrations.config.ts",
    "migration:generate": "yarn typeorm:cli-with-configs migration:generate",
    "migration:run": "yarn typeorm:cli-with-configs migration:run",
    "migration:revert": "yarn typeorm:cli-with-configs migration:revert",
    "migration:create": "typeorm migration:create",
    "console": "ts-node --project ./tsconfig.json ./src/core/scripts/console.ts",
    "docker:dev": "sh docker/compose.sh console"
  },
  "dependencies": {
    "@actograph/core": "file:../packages/core",
    "@nestjs/common": "^9.0.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.1",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/cron": "^1.7.3",
    "@types/multer": "^1.4.7",
    "@types/validator": "^13.7.2",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "env-paths": "^3.0.0",
    "exceljs": "^4.3.0",
    "fs": "0.0.1-security",
    "googleapis": "^82.0.0",
    "int64-buffer": "^1.0.1",
    "joiful": "^3.0.2",
    "jsonschema": "^1.4.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "rxjs-compat": "^6.6.7",
    "slugify": "^1.6.0",
    "typeorm": "^0.3.9",
    "uid": "^2.0.0",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.1",
    "@types/better-sqlite3": "^7.6.11",
    "@types/express": "^4.17.13",
    "esbuild": "^0.24.0",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "29.0.1",
    "@types/node": "^22.13.10",
    "@types/passport-google-oauth20": "^2.0.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "inquirer": "^8.2.0",
    "jest": "29.0.1",
    "prettier": "^2.5.1",
    "supertest": "^4.0.2",
    "ts-jest": "29.0.1",
    "ts-loader": "^6.2.1",
    "ts-node": "10.8.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.9.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
