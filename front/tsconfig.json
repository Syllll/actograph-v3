{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "boot/*": ["src/boot/*"],
      "@boot/*": ["src/boot/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@css/*": ["src/components/*"],
      "@i18n/*": ["src/i18n/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@router/*": ["src/router/*"],
      "@services/*": ["src/services/*"],
      "@utils/*": ["src/utils/*"],
      "@lib-improba/*": ["lib-improba/*"]
    }
  },
  "exclude": [
    "./dist",
    "./.quasar",
    "./node_modules",
    "./src-capacitor",
    "./src-cordova",
    "./src-electron",
    "./quasar.config.*.temporary.compiled*"
  ]
}
